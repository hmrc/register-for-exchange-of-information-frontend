#!/bin/bash

echo ""
echo "Applying migration NoRecordsMatched"

echo "Adding routes to conf/app.routes"
echo "" >> ../conf/app.routes
echo "GET        /noRecordsMatched                       controllers.NoRecordsMatchedController.onPageLoad()" >> ../conf/app.routes

echo "Adding messages to conf.messages"
echo "" >> ../conf/messages.en
echo "noRecordsMatched.title = noRecordsMatched" >> ../conf/messages.en
echo "noRecordsMatched.heading = noRecordsMatched" >> ../conf/messages.en

echo "Migration NoRecordsMatched completed"
